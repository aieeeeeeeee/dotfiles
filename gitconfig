[user]
	name = aieeeeeeeee
	email = to_change

[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[http]
	sslVerify = true

[core]
	editor = vim
	excludesfile = $HOME/.gitignore
        pager = less -FRSX
        whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[color]
        diff = auto
        status = auto
        branch = auto
        ui = auto
        decorate = full
	interactive = auto
	pager = true
[format]
        pretty = %Cred%h %Cgreen(%cd) %Creset%s%C(yellow)%d %C(bold cyan)<%an>%Creset
[log]
        date = iso
        abbrevCommit = false

[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
 
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
 
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[push]
	default = simple
